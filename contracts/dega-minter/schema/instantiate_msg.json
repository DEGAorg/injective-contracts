{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "instantiate_msg",
  "type": "object",
  "required": [
    "collection_params",
    "cw721_contract_label",
    "minter_params"
  ],
  "properties": {
    "collection_params": {
      "$ref": "#/definitions/CollectionParams"
    },
    "cw721_contract_admin": {
      "type": [
        "string",
        "null"
      ]
    },
    "cw721_contract_label": {
      "type": "string"
    },
    "minter_params": {
      "$ref": "#/definitions/MinterParams_for_DegaMinterParams"
    }
  },
  "additionalProperties": false,
  "definitions": {
    "Coin": {
      "type": "object",
      "required": [
        "amount",
        "denom"
      ],
      "properties": {
        "amount": {
          "$ref": "#/definitions/Uint128"
        },
        "denom": {
          "type": "string"
        }
      }
    },
    "CollectionInfo_for_RoyaltyInfoResponse": {
      "type": "object",
      "required": [
        "creator",
        "description",
        "image"
      ],
      "properties": {
        "creator": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "external_link": {
          "type": [
            "string",
            "null"
          ]
        },
        "image": {
          "type": "string"
        },
        "royalty_info": {
          "anyOf": [
            {
              "$ref": "#/definitions/RoyaltyInfoResponse"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "additionalProperties": false
    },
    "CollectionParams": {
      "type": "object",
      "required": [
        "code_id",
        "info",
        "name",
        "symbol"
      ],
      "properties": {
        "code_id": {
          "description": "The collection code id",
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "info": {
          "$ref": "#/definitions/CollectionInfo_for_RoyaltyInfoResponse"
        },
        "name": {
          "type": "string"
        },
        "symbol": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "Decimal": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal(1_000_000_000_000_000_000) == 1.0\n\nThe greatest possible value that can be represented is 340282366920938463463.374607431768211455 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "DegaMinterConfigSettings": {
      "type": "object",
      "required": [
        "minting_paused",
        "signer_pub_key"
      ],
      "properties": {
        "minting_paused": {
          "type": "boolean"
        },
        "signer_pub_key": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "DegaMinterParams": {
      "type": "object",
      "required": [
        "dega_minter_settings",
        "initial_admin"
      ],
      "properties": {
        "dega_minter_settings": {
          "$ref": "#/definitions/DegaMinterConfigSettings"
        },
        "initial_admin": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "MinterParams_for_DegaMinterParams": {
      "description": "Common params for all minters used for storage",
      "type": "object",
      "required": [
        "creation_fee",
        "extension",
        "min_mint_price",
        "mint_fee_bps"
      ],
      "properties": {
        "creation_fee": {
          "description": "The minter code id",
          "allOf": [
            {
              "$ref": "#/definitions/Coin"
            }
          ]
        },
        "extension": {
          "$ref": "#/definitions/DegaMinterParams"
        },
        "min_mint_price": {
          "$ref": "#/definitions/Coin"
        },
        "mint_fee_bps": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      },
      "additionalProperties": false
    },
    "RoyaltyInfoResponse": {
      "type": "object",
      "required": [
        "payment_address",
        "share"
      ],
      "properties": {
        "payment_address": {
          "type": "string"
        },
        "share": {
          "$ref": "#/definitions/Decimal"
        }
      },
      "additionalProperties": false
    },
    "Uint128": {
      "description": "A thin wrapper around u128 that is using strings for JSON encoding/decoding, such that the full u128 range can be used for clients that convert JSON numbers to floats, like JavaScript and jq.\n\n# Examples\n\nUse `from` to create instances of this and `u128` to get the value out:\n\n``` # use cosmwasm_std::Uint128; let a = Uint128::from(123u128); assert_eq!(a.u128(), 123);\n\nlet b = Uint128::from(42u64); assert_eq!(b.u128(), 42);\n\nlet c = Uint128::from(70u32); assert_eq!(c.u128(), 70); ```",
      "type": "string"
    }
  }
}
