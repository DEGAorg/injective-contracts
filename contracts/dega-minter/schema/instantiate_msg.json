{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "instantiate_msg",
  "type": "object",
  "required": [
    "collection_params",
    "cw721_contract_label",
    "minter_params"
  ],
  "properties": {
    "collection_params": {
      "$ref": "#/definitions/CollectionParams"
    },
    "cw721_contract_admin": {
      "type": [
        "string",
        "null"
      ]
    },
    "cw721_contract_label": {
      "type": "string"
    },
    "minter_params": {
      "$ref": "#/definitions/DegaMinterParams"
    }
  },
  "additionalProperties": false,
  "definitions": {
    "CollectionInfoResponse": {
      "type": "object",
      "required": [
        "creator",
        "description",
        "image"
      ],
      "properties": {
        "creator": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "external_link": {
          "type": [
            "string",
            "null"
          ]
        },
        "image": {
          "type": "string"
        },
        "royalty_settings": {
          "anyOf": [
            {
              "$ref": "#/definitions/RoyaltySettingsResponse"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "additionalProperties": false
    },
    "CollectionParams": {
      "type": "object",
      "required": [
        "code_id",
        "info",
        "name",
        "symbol"
      ],
      "properties": {
        "code_id": {
          "description": "The collection code id",
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "info": {
          "$ref": "#/definitions/CollectionInfoResponse"
        },
        "name": {
          "type": "string"
        },
        "symbol": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "Decimal": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal(1_000_000_000_000_000_000) == 1.0\n\nThe greatest possible value that can be represented is 340282366920938463463.374607431768211455 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "DegaMinterConfigSettings": {
      "type": "object",
      "required": [
        "minting_paused",
        "signer_pub_key"
      ],
      "properties": {
        "minting_paused": {
          "type": "boolean"
        },
        "signer_pub_key": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "DegaMinterParams": {
      "type": "object",
      "required": [
        "dega_minter_settings",
        "initial_admin"
      ],
      "properties": {
        "dega_minter_settings": {
          "$ref": "#/definitions/DegaMinterConfigSettings"
        },
        "initial_admin": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "RoyaltySettingsResponse": {
      "type": "object",
      "required": [
        "payment_address",
        "share"
      ],
      "properties": {
        "payment_address": {
          "type": "string"
        },
        "share": {
          "$ref": "#/definitions/Decimal"
        }
      },
      "additionalProperties": false
    }
  }
}
